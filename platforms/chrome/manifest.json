{
    "manifest_version": 2,
    "name": "Augus-HisHistory",
    "description": "彼の思い出を記録しちゃおう",
    "version": "0.0.0.a",
    "app": {
        "background": {
            "scripts": [
                "background.js"
            ]
        }
    },
    "icons": {
        "128": "icon.png"
    },
    "permissions": [
        "clipboardWrite",
        "clipboardRead",
        "unlimitedStorage"
    ],
    "offline_enabled": true,
    "optional_permissions": [],
    "externally_connectable": {
        "ids": [],
        "matches": [],
        "accepts_tls_channel_id": false
    }
}